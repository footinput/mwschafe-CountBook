<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_view_relative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mwschafe_sizebook.viewRelative">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/nameNew"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Date:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_below="@+id/dateF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:text="Neck:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_below="@+id/neckF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="26dp"
        android:text="Bust:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_below="@+id/bustF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:text="Chest:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Waist:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView18"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_marginTop="25dp"
        android:text="Hip:"
        android:layout_below="@+id/waistF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/hipF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Inseam:" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Name:"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/neckF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Neck:"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignLeft="@+id/dateF"
        android:layout_alignStart="@+id/dateF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Comment:"
        android:layout_alignBaseline="@+id/commentF"
        android:layout_alignBottom="@+id/commentF"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/commentF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:layout_marginTop="23dp"
        android:text="Comment:"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_below="@+id/inseamF"
        android:layout_toRightOf="@+id/textView20"
        android:layout_toEndOf="@+id/textView20" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nameNew"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Name:"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/commentF"
        android:layout_alignStart="@+id/commentF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dateF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Date:"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignLeft="@+id/nameNew"
        android:layout_alignStart="@+id/nameNew" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bustF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Bust:"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignRight="@+id/dateF"
        android:layout_alignEnd="@+id/dateF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chestF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Chest:"
        android:layout_alignBaseline="@+id/textView16"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignLeft="@+id/neckF"
        android:layout_alignStart="@+id/neckF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/waistF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Waist:"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/nameNew"
        android:layout_alignEnd="@+id/nameNew" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/inseamF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Inseam:"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignLeft="@+id/waistF"
        android:layout_alignStart="@+id/waistF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/hipF"
        android:textSize="18sp"
        android:textColor="@android:color/background_light"
        android:text="Hip:"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignBottom="@+id/textView18"
        android:layout_alignLeft="@+id/inseamF"
        android:layout_alignStart="@+id/inseamF" />

    <Button
        android:text="Delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="29dp"
        android:layout_marginEnd="29dp"
        android:id="@+id/deleteRelativeButton" />

    <Button
        android:text="Edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/neckF"
        android:layout_toEndOf="@+id/neckF"
        android:id="@+id/editRelative" />

</RelativeLayout>
